{
  "name": "banrural-csv-parser",
  "version": "1.0.3",
  "description": "Parser for Banrural bank statements in CSV format",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "banrural",
    "parser",
    "csv",
    "bank",
    "guatemala"
  ],
  "author": "SoftLari",
  "license": "GPL-3.0-only",
  "dependencies": {
    "csv-parse": "5.6.0",
    "csv-stringify": "6.5.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "tsx": "4.19.3",
    "typescript": "~5.4.2",
    "vite": "^5.4.14",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "3.0.9"
  }
}
